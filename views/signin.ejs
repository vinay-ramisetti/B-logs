<!DOCTYPE html>
<html lang="en">
<head>
   <%- include("./partials/head") %>
  <title>SignIn</title>
</head>
<body>
  <%- include("./partials/nav") %>
  <div class="container">
    <form action="/user/signin" method="post">
      <h2 class="mt-3" style="color: blue;">Sign In</h2>
  
      <label for="email" class="form-label">Email:</label>
      <input type="text" id="email" class="form-control" aria-describedby="email" name="email">
  
      <label for="password" class="form-label">Password:</label>
      <input type="password" id="password" class="form-control" aria-describedby="password" name="password">
       
      <div class="col-auto mt-3">
        <button type="submit" class="btn btn-primary mb-3">Submit</button>
      </div>
    </form>

  </div>
  <%- include("./partials/scripts") %>
</body>
</html>